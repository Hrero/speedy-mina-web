page{
    background: #ffffff;
}
.zg-pageIndex {
  flex: 1;
  font-family: PingFangSC-Regular;
  background: #ffffff;
  box-sizing: border-box;
  overflow: hidden;
  /* padding-top: 100rpx; */
}
.lai{
    height:100rpx;
    box-shadow: 0rpx 10rpx 20rpx #eff0f2;
}
.zg-pageIndex .homeList {
    background: #f6f7f9;
    box-sizing: border-box;
    position: relative;
    margin-top: 60rpx;
    width: 100%;
    overflow: hidden;
    column-count:2;
    column-gap:5rpx;
    padding: 0 10rpx;
  }
.zg-pageIndex .homeList .item-li {
    break-inside: avoid;
    background: #ffffff;
    /* width: calc(50% - 8rpx); */
    margin: 0rpx 5rpx 10rpx 5rpx;
    box-sizing: border-box;
    border-radius: 8rpx;
    padding-bottom: 30rpx;
    /* overflow: hidden; */
}
.des-item{
    padding: 10rpx 20rpx;
    line-height: 40rpx;
    font-size: 28rpx;
    font-weight:bold;
    color: #2a2a2a;
}
.zg-pageIndex .homeList .item-li .list-img .listImg {
    width: 100%;
    border-top-left-radius: 8rpx;
    border-top-right-radius: 8rpx;
}
.head{
    /* width: 100%; */
    display: flex;
    justify-content: space-between;
    padding:0 20rpx;
    margin-top:10rpx; 
}
.headImg{
    display: flex;
    justify-content: center;
    align-items: center;
}
.name{
    font-size: 24rpx;
    margin: 0 5rpx;
}
.image{
    width: 50rpx;
    height:50rpx;
    border-radius: 50%;
}
.image image{
    width: 100%;
    height: 100%;
    border-radius: 50%;
}
.zan{
    width: 30rpx;
    height:30rpx;
    border-radius: 50%;
    margin:0 10rpx 0 5rpx;
}
.zan-wrap{
    display: flex;
    justify-content: center;
    align-items: center;
    font-size:28rpx;
}
.headWrap{
    background: #ffffff;
    width: 92%;
    height:100rpx;
    padding:0 4%;
    display: flex;
    justify-content: center;
    align-items: center;
    /* position: fixed;
    top:0;
    left:0; */
    z-index: 100;
}
.autoFocio{
    width: 90%;
    box-sizing: border-box;
    border-radius: 30rpx;
    height:65rpx;
    line-height: 65rpx;
    font-size:30rpx;
    color: #a0a0a0;
    margin: 0 auto;
    background: #f5f5f5;
    position: relative;
}
.autoInput{
    padding-left: 70rpx;
    height: 65rpx;
    line-height: 65rpx;
}
.small{
    width: 35rpx;
    height: 35rpx;
    background: url(https://static2.zugeliang01.com/lease/img/6acced40-9c81-11e9-a88d-77a7a2cf7dec.png) no-repeat;
    background-size: 35rpx 35rpx;
    position: absolute;
    top:15rpx;
    left:25rpx;    
}
.searchBtn{
    color: #cccccc;
    font-size: 28rpx;
}
.searchBtn-down{
    padding: 30rpx;
    font-size: 30rpx;
}
.search-body{
    display: flex;
    flex-direction: row;
    margin:20rpx 0;
    position: relative;
    flex-wrap: wrap;
}
.search-item{
    background: #f5f5f5;
    padding: 18rpx 30rpx;
    border-radius: 40rpx;
    margin-right: 30rpx;
    margin-bottom: 20rpx; 
    display: flex;
    justify-content: center;
    align-items: center;
}
.listicon{
    width: 35rpx;
    height: 35rpx;
}
.delete{
    position: absolute;
    top: 22rpx;
    right: -5rpx;
    width: 35rpx;
    height: 35rpx;
}
.home-bottom-tab{
    height:80rpx;
    display: flex;
    justify-content: center;
    align-items: center;
    background: #ffffff;
    font-size: 32rpx;
    padding: 10rpx 0;
    color: #646464;
}
.home-bottom-tab view{
    display: flex;
    justify-content: center;
    align-items: center;
    box-sizing: border-box;
    border: 4rpx solid #ffffff;
}
.cbg{
    margin-left:90rpx;
}
.home-bottom-tab .fans-tab {
    border-bottom: 4rpx solid #EFAB17;
    padding:10rpx 0rpx;
    color: #000000;
}
.goback{
    display: flex;
    justify-items: center;
    align-items: center;
    position: absolute;
    top:16rpx;
    right: 10rpx;
    z-index: 1001;
}
.gobackdelete{
    width: 35rpx;
    height: 35rpx;
}
.txtCenter{
    background: #f7f7f7;
