.user-edit{
    height:1200rpx;
    background: #ffffff;
    width: 100%;
}
.handleChange{
    color: #ffffff;
    width: 100%;
    height:80rpx;
    line-height: 80rpx;
    text-align: center;
    background: #d81e06;
    position: fixed;
    bottom:0;
    left:0;
}
.inputWrap{
    padding:0 30rpx;
    font-size: 28rpx;
}

.swiper-item, .swiper, .swiper-wrap, .slide-image{
    background: #ffffff;
    width: 100%;
}
.section-btn{
    color: #ffffff;
    width: 90%;
    height:80rpx;
    line-height: 80rpx;
    text-align: center;
    background: red;
    border-radius: 10rpx;
    margin: 0 auto;
}
.personZl{
    padding: 100rpx 30rpx 0;
    background: #ffffff;
    font-size: 30rpx;
}
.line{
    padding: 20rpx 0;
    display: flex;
    justify-content: center;
    align-items: center;
    border-bottom:1rpx solid #e8e8e8;
}
.left{
    width:200rpx;
}
.right{
    flex:1;
    width: 500rpx;
}
.personTxt{
    padding: 0 30rpx;
    line-height: 80rpx;
    font-size:28rpx;
    color: #cccccc;
}
.left{
    color: #cccccc;
}
.left image{
    height:100rpx;
    width:100rpx;
    border-radius: 50%;
}
.sign{
    border-bottom:0rpx solid #e8e8e8;
    padding: 60rpx 0;
}
.rightJT{
    width: 20rpx;
    height: 20rpx;
}

.search-body{
    display: flex;
    flex-direction: row;
    justify-content: center;
    align-items: center;
    margin:20rpx 0;
    position: relative;
    flex-wrap: wrap;
}
.search-item{
    background: #f5f5f5;
    padding: 18rpx 30rpx;
    border-radius: 40rpx;
    margin-right: 30rpx;
    margin-bottom: 20rpx; 
    display: flex;
    justify-content: center;
    align-items: center;
}
.activeT{
    background: #d81e06;
    color: #ffffff;
}
.delete{
    position: absolute;
    top: 22rpx;
    right: -5rpx;
    width: 35rpx;
    height: 35rpx;
}
.txt-oo{
    font-size: 24rpx;
    text-align: center;
    margin: 30rpx auto 200rpx;
}
page{
    background: #ffffff;
